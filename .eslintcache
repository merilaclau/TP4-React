adasAll.push(temporadas)\n        }\n    }, [totalSeason])\n\n    const handleClick = (e) => {\n        setSeason(e.target.value)\n    }\n\n\n\n    return (\n        <div >\n            <div className=\"seasons\">\n                <select className=\"season-select\" name=\"season\" id=\"\">\n                    {temporadasAll.map((item, i) => {\n                        return <option onClick={handleClick} /* key={i} value={i + 1} */>{item}</option>\n                    })}\n                </select>\n            </div>\n            <div className=\"episodes\">\n                {results.episodes && results.episodes.map((item, i) => {\n                    return (<div className=\"episodes-results\" key={i}>\n                        <img src={`${ImgUrl}${item.still_path}`} alt=\"\" />\n                        <div className=\"episodes-details\">\n                            <p> <span>EP{item.episode_number}</span> {item.name}</p>\n                            <p className=\"episode-overview\">{item.overview}</p>\n                        </div>\n                    </div>)\n                })}\n            </div>\n        </div>\n    )\n}\n\nexport default Episodios","/Users/mariahayward/Desktop/AdaMoviesFinal/TP4-React/src/components/Similar/Similar.js",["109"],{"ruleId":"110","severity":1,"message":"111","line":1,"column":17,"nodeType":"112","messageId":"113","endLine":1,"endColumn":25},{"ruleId":"110","severity":1,"message":"114","line":1,"column":27,"nodeType":"112","messageId":"113","endLine":1,"endColumn":36},{"ruleId":"115","severity":1,"message":"116","line":23,"column":30,"nodeType":"117","endLine":23,"endColumn":82},{"ruleId":"118","severity":1,"message":"119","line":16,"column":28,"nodeType":"120","messageId":"121","endLine":16,"endColumn":30},{"ruleId":"118","severity":1,"message":"119","line":20,"column":28,"nodeType":"120","messageId":"121","endLine":20,"endColumn":30},{"ruleId":"122","replacedBy":"123"},{"ruleId":"124","replacedBy":"125"},{"ruleId":"122","